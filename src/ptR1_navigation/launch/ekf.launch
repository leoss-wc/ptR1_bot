<launch>

  <!-- ðŸ§­ TF Broadcaster à¹à¸šà¸šà¸¡à¸µ timestamp à¸ªà¸” -->
  <node name="dynamic_tf_broadcaster" pkg="test_package" type="dynamic_static_tf.py" output="screen"/>

  <!-- ðŸ”Œ ROSSerial (à¸ªà¹ˆà¸‡ IMU, Mag, Odom) -->
